defprimitive u8 uint 1;
defprimitive u16 uint 2;
defprimitive u32 uint 4;
defprimitive char character 1;

iconst FooConst = 99;
iconst BarConst = -1;

import "testdata/other_file"

generator c : enable_reflection;

// Comments working

struct moo {
	s32* po;
	void** bar; 			// great huh
    __cstring<char> text;
	float*[3] array;
	float[2,2] array2;
}

struct fisk : base(fisk_base), comment(text="This is a great struct!") {
	s32* a;
	float b;
	moo c;
	selfref[3] x;
	s32[3] y;
}

struct selfref {
	selfref *next;
	selfref *prev;
}

struct aligntest {
    u32 b;
    s8 a;
}

struct aligntest2 {
    aligntest a;
    aligntest b;
}
